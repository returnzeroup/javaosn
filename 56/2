import java.io.*;
 
class MainClass {
 
    public static void main(String[] args) throws Exception {
        FileWriter fw = new FileWriter( "sample1.txt" );
        fw.close();
 
        FileReader fr = new FileReader( "sample2.txt" );
        fr.close();
 
    }
}

