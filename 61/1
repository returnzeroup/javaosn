class MyClass {
 
    public int addAll(int firstNumber, int secondNumber)
    {
        return firstNumber + secondNumber;
    }
 
}

/*
class MyClass {
 
    public int addAll(int firstNumber, int secondNumber) {
        return firstNumber + secondNumber;
    }
 
    public int addAll(int firstNumber, int secondNumber, int thirdNumber) {
        return firstNumber + secondNumber + thirdNumber;
    }
}
*/

